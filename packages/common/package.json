{
  "name": "@drawink/common",
  "version": "0.18.0",
  "private": true,
  "type": "module",
  "types": "./dist/types/common/src/index.d.ts",
  "main": "./dist/prod/index.js",
  "module": "./dist/prod/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/common/src/index.d.ts",
      "development": "./dist/dev/index.js",
      "production": "./dist/prod/index.js",
      "default": "./dist/prod/index.js"
    },
    "./*": {
      "types": "./dist/types/common/src/*.d.ts",
      "development": "./dist/dev/index.js",
      "production": "./dist/prod/index.js",
      "default": "./dist/prod/index.js"
    }
  },
  "files": ["dist/*"],
  "description": "Drawink common functions, constants, etc.",
  "dependencies": {
    "react": "19.0.3"
  },
  "keywords": ["drawink", "drawink-utils"],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all",
      "not safari < 12",
      "not kaios <= 2.5",
      "not edge < 79",
      "not chrome < 70",
      "not and_uc < 13",
      "not samsung < 10"
    ],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "scripts": {
    "gen:types": "rimraf types && tsc --project tsconfig.build.json --skipLibCheck --noImplicitAny false",
    "build": "bun run build:esm",
    "build:esm": "rimraf dist && bun ../../scripts/buildBase.ts && bun run gen:types"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.3",
    "react-dom": "^18.2.0 || ^19.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3"
  }
}
