{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "emitDeclarationOnly": true,
    "paths": {
      "@drawink/common": [
        "./common/src/index.ts"
      ],
      "@drawink/common/*": [
        "./common/src/*"
      ],
      "@drawink/element": [
        "./element/src/index.ts"
      ],
      "@drawink/element/*": [
        "./element/src/*"
      ],
      "@drawink/drawink": [
        "./drawink/index.tsx"
      ],
      "@drawink/drawink/*": [
        "./drawink/*"
      ],
      "@drawink/math": [
        "./math/src/index.ts"
      ],
      "@drawink/math/*": [
        "./math/src/*"
      ],
      "@drawink/utils": [
        "./utils/src/index.ts"
      ],
      "@drawink/utils/*": [
        "./utils/src/*"
      ]
    }
  }
}